@using MvcStokTakip.Models.Entity
@model List<tblsatis>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>ÜRÜN</th>
        <th>PERSONEL</th>
        <th>MÜŞTERİ</th>
        <th>FİYAT</th>
        <th>TARİH</th>
    </tr>

    @foreach (var x in Model)
    {
        <tr>
            <td>@x.id</td>
            <td>@x.tblurun.ad</td>
            <td>@x.tblpersonel.ad @x.tblpersonel.soyad</td>
            <td>@x.tblmusteri.ad @x.tblmusteri.soyad</td>
            <td>@x.fiyat</td>
            <td>@x.tarih</td>
        </tr>
    }
</table>

<a href="/Satis/YeniSatis/" class="btn btn-primary">Yeni Satış</a>

